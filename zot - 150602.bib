	number = {12},
	journaltitle = {Journal of Gastrointestinal Surgery},
	shortjournal = {J Gastrointest Surg},
	author = {Shirakawa, Sachiyo and Matsumoto, Ippei and Toyama, Hirochika and Shinzeki, Makoto and Ajiki, Tetsuo and Fukumoto, Takumi and Ku, Yonson},
	urldate = {2014-03-16},
	date = {2012-12},
