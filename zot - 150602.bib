@article{palani_velu_serum_2013,
	title = {Serum amylase on the night of surgery predicts clinically significant pancreatic fistula after pancreaticoduodenectomy},
	issn = {1477-2574},
	doi = {10.1111/hpb.12184},
	abstract = {{OBJECTIVES}: Drainage after pancreaticoduodenectomy ({PD}) remains controversial because the risk for uncontrolled postoperative pancreatic fistula ({POPF}) must be balanced against the potential morbidity associated with prolonged and possibly unnecessary drainage. This study investigated the utility of the level of serum amylase on the night of surgery [postoperative day ({PoD}) 0 serum amylase] to predict {POPF}.
{METHODS}: A total of 185 patients who underwent {PD} were studied. Occurrences of {POPF} were graded using the International Study Group on Pancreatic Fistula ({ISGPF}) classification. Receiver operating characteristic ({ROC}) analysis identified a threshold value of {PoD} 0 serum amylase associated with clinically significant {POPF} ({ISGPF} Grades B and C) in a test cohort (n = 45). The accuracy of this threshold value was then tested in a validation cohort (n = 140).
{RESULTS}: Overall, 43 (23.2\%) patients developed clinically significant {POPF}. The threshold value of {PoD} 0 serum amylase for the identification of clinically significant {POPF} was ≥130 {IU}/l (P = 0.003). Serum amylase of {\textless}130 {IU}/l had a negative predictive value of 88.8\% for clinically significant {POPF} (P {\textless} 0.001). Serum amylase of ≥130 {IU}/l on {PoD} 0 and a soft pancreatic parenchyma were independent risk factors for clinically significant {POPF}.
{CONCLUSIONS}: Postoperative day 0 serum amylase of {\textless}130 {IU}/l allows for the early and accurate categorization of patients at least risk for clinically significant {POPF} and may identify patients suitable for early drain removal.},
	journaltitle = {{HPB}: the official journal of the International Hepato Pancreato Biliary Association},
	shortjournal = {{HPB} (Oxford)},
	author = {Palani Velu, Lavanniya K and Chandrabalan, Vishnu V and Jabbar, Salman and {McMillan}, Donald C and {McKay}, Colin J and Carter, C Ross and Jamieson, Nigel B and Dickson, Euan J},
	date = {2013-11-07},
	pmid = {24246024}
}

@article{batterham_modeling_1999,
