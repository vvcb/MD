	keywords = {aerobic fitness, all-cause mortality, Body Composition, Body Weight, cardiorespiratory fitness, cardiovascular-disease mortality, energy-expenditure, Exercise, men, obese children, Obesity, physical   activity, physical-fitness, statistical considerations, women},
	file = {Goran et al. - 2000 - Total body fat does not influence maximal aerobic .pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\UVDV4IM9\\Goran et al. - 2000 - Total body fat does not influence maximal aerobic .pdf:application/pdf}
}

