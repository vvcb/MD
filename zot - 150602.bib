@article{epstein_analysis_1998,
	title = {Analysis of impaired exercise capacity in patients with cirrhosis},
	volume = {43},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/9724156},
	abstract = {Exercise limitation in cirrhosis is typically attributed to a cirrhotic myopathy (without impaired oxygen utilization) and/or a cardiac chronotropic dysfunction. We performed symptom-limited cardiopulmonary exercise testing in 19 cirrhotics without confounding variables (cardiopulmonary disease, beta blockade, anemia, smoking). Twelve concurrently exercised patients without cirrhosis and with normal resting pulmonary function were controls. Oxygen consumption ({VO}2) at peak exercise, at anaerobic threshold ({VO}2-{AT}), work rate ({WR}), and heart rate ({HR}) were measured. Cirrhotics had significantly lower peak {WR} (73+/-4 vs 107+/-7\% predicted, p {\textless} 0.001), {VO}2 (72+/-4 vs 98+/-5\% predicted, P {\textless} 0.001), {VO}2-{AT} (53+/-4 vs 71+/-5\% predicted peak {VO}2, P {\textless} 0.01), {HR} (83+/-2 vs 91+/-2\% predicted, P {\textless} 0.01) and were more likely to have chronotropic dysfunction (peak {HR} {\textless} 85\% predicted). Six cirrhotics had normal aerobic capacity (peak {VO}2 {\textgreater} 80\% predicted), while 13 were abnormal. The abnormals had an earlier {AT} (46+/-2 vs 67+/-3\% predicted peak {VO}2, P {\textless} 0.05) but no difference in peak {HR} percent predicted was found. In conclusion, two thirds of cirrhotics, without confounding factors, have significantly reduced aerobic capacity. Cirrhotic myopathy (without impaired O2 utilization) and cardiac chronotropic dysfunction do not adequately account for the observed decrease in aerobic capacity.},
	pages = {1701--7},
	number = {8},
	journaltitle = {Dig Dis Sci},
	author = {Epstein, S K and Ciubotaru, R L and Zilberberg, M D and Kaplan, L M and Jacoby, C and Freeman, R and Kaplan, M M},
	date = {1998-08},
	file = {Epstein et al. - 1998 - Analysis of impaired exercise capacity in patients.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\JJUGA6AX\\Epstein et al. - 1998 - Analysis of impaired exercise capacity in patients.pdf:application/pdf}
}

@article{west_effects_2014,
	title = {The effects of neoadjuvant chemoradiotherapy on physical fitness and morbidity in rectal cancer surgery patients},
	volume = {40},
	issn = {0748-7983},
	url = {http://www.sciencedirect.com/science/article/pii/S0748798314003862},
	doi = {10.1016/j.ejso.2014.03.021},
	abstract = {{AbstractBackground}
Neoadjuvant chemoradiotherapy ({NACRT}) followed by surgery for resectable locally advanced rectal cancer improves outcome compared with surgery alone. Our primary hypothesis was that {NACRT} impairs objectively-measured physical fitness. We also wished to explore the relationship between fitness and postoperative outcome.
Method
In an observational study, we prospectively studied 27 consecutive patients, of whom 25 undertook cardiopulmonary exercise testing ({CPET}) 2 weeks before and 7 weeks after standardized {NACRT}, then underwent surgery. In-hospital post-operative morbidity and mortality were recorded. Patients were followed up to 1 year for mortality. Data was analysed blind to clinical details. Receiver-operating characteristic ({ROC}) analysis defined the predictive value of {CPET} for in-hospital morbidity at day 5.
Results
Oxygen uptake ( V ˙ O 2 in ml kg−1 min−1) at estimated lactate threshold ( θ ˆ L ) and at peak exercise ( V ˙ O 2 at peak in ml kg−1 min−1) both significantly decreased post-{NACRT}: V ˙ O 2 at θ ˆ L 12.1 (pre-{NACRT}) vs. 10.6 (post-{NACRT}), p \&lt; 0.001 (95\%{CI} −1.7, −1.2); V ˙ O 2 at peak 18.1 vs. 16.7, p \&lt; 0.001 (95\%{CI} −3.1, −1.0). Optimal V ˙ O 2 at θ ˆ L and peak pre-{NACRT} for predicting postoperative morbidity were 12.0 and 18.1 ( V ˙ O 2 at θ ˆ L – {AUC} = 0.71, 77\% sensitive and 75\% specific; V ˙ O 2 at peak – {AUC} = 0.75, 78\% sensitive and 76\% specific). Optimal V ˙ O 2 at θ ˆ L and peak post-{NACRT} for predicting postoperative morbidity were 10.7 and 16.7 ( V ˙ O 2 at θ ˆ L – {AUC} = 0.72, 77\% sensitive and 83\% specific; V ˙ O 2 at peak – {AUC} = 0.80, 85\% sensitive and 83\% specific).
Conclusion
{NACRT} before major rectal cancer surgery significantly decreased physical fitness as assessed by {CPET}.
Trials Registry Number
{NCT}01334593.},
	pages = {1421--1428},
	number = {11},
	journaltitle = {European Journal of Surgical Oncology ({EJSO})},
	shortjournal = {European Journal of Surgical Oncology ({EJSO})},
	author = {West, M. A. and Loughney, L. and Barben, C. P. and Sripadam, R. and Kemp, G. J. and Grocott, M. P. W. and Jack, S.},
	urldate = {2015-05-05},
	date = {2014-11},
	keywords = {Cancer surgery, Cardiopulmonary exercise testing, Morbidity, Neoadjuvant chemoradiotherapy, Physical fitness, rectal cancer},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\24KIMW4W\\West et al. - 2014 - The effects of neoadjuvant chemoradiotherapy on ph.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\P2KCN9NS\\S0748798314003862.html:text/html}
