	pmcid = {PMC3508270},
	file = {Shirakawa et al. - 2012 - Pancreatic Volumetric Assessment as a Predictor of.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\WWP799IB\\Shirakawa et al. - 2012 - Pancreatic Volumetric Assessment as a Predictor of.pdf:application/pdf}
