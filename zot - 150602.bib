	keywords = {American Indians, diabetes mellitus, echocardiography, ventricles},
	file = {Collis et al. - 2001 - Relations of Stroke Volume and Cardiac Output to B.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\8I4RPESU\\Collis et al. - 2001 - Relations of Stroke Volume and Cardiac Output to B.pdf:application/pdf;Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\av2t1t3u.default\\zotero\\storage\\8A6B2WGR\\820.html:text/html}
