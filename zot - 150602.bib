@article{shirahatti_palliative_1997,
	title = {Palliative surgery in malignant obstructive jaundice: prognostic indicators of early mortality},
	volume = {42},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/9276557},
	abstract = {A prospective analysis of 50 patients undergoing palliative bypass surgery for incurable malignant obstructive jaundice was carried out in an attempt to identify factors predicting post-operative mortality. Five clinical and nine laboratory parameters were studied. Fourteen patients died within 30 days of surgery. It was seen that levels of haemoglobin, hematocrit and serum albumin levels were significantly lower while serum bilirubin was significantly higher in patients who died compared with the survivors. Patients having Hb {\textless} 10 Gm/{dL}, serum bilirubin {\textgreater} 350 micromol/{dL}, serum albumin {\textless} 2.5 g/{dL} and prothrombin index {\textless} 60\% exhibited a higher percentage of mortality. On multivariate analysis, levels of haemoglobin, serum albumin and serum bilirubin could be used to independently predict the outcome with an accuracy of 86\%. This was validated by prospectively applying the regression equation derived from the first 30 patients to the next 20 patients. It was seen that the predicted outcome correlated with the actual outcome with a correlation coefficient of 0.5098 (P = 0.01). It is concluded that in patients with high bilirubin, low haemoglobin and low albumin levels palliative surgical procedures carry a high risk of post-operative mortality and non-surgical methods may be more suitable.},
	pages = {238--43},
	number = {4},
	journaltitle = {J R Coll Surg Edinb},
	author = {Shirahatti, R G and Alphonso, N and Joshi, R M and Prasad, K V and Wagle, P K},
	date = {1997-08}
}

