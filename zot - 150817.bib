@article{marla_factors_2013,
	title = {Factors influencing postoperative length of hospital stay after breast cancer surgery.},
	volume = {22},
	rights = {Copyright (c) 2012 Elsevier Ltd. All rights reserved.},
	doi = {10.1016/j.breast.2012.07.009},
	abstract = {As part of a feasibility study to restructure the breast cancer services in Glasgow, factors influencing 'postoperative length of stay' ({LOS}) and bed utilisation in patients undergoing surgery for breast cancer were examined. Data  for patients admitted at five hospitals between March 2007 and February 2008 was  collected prospectively. Age, socio-demographic and clinico-pathologic factors were recorded. Independent affects of variables predicting prolonged {LOS} were assessed using binary logistic regression analysis. Of the 519 women, 252(49\%) had screen-detected cancers with a median {LOS} of 1 day while 267(51\%) had symptomatic cancers with a median {LOS} of 4 days (p {\textless} 0.001). On multivariate analysis, axillary procedure performed independently influenced prolonged {LOS} in  both screen-detected and symptomatic cancers. In symptomatic cancers, comorbidities and deprivation also had some influence. While mastectomy with or without axillary surgery utilised 51\% of the bed days, a further 20\% were utilised by patients having re-operations. This study has helped in the planning  of ambulatory surgery services and inpatient bed requirements for patients undergoing breast cancer surgery in Glasgow.},
	pages = {289--294},
	number = {3},
	journaltitle = {Breast (Edinburgh, Scotland)},
	shortjournal = {Breast},
	author = {Marla, Sekhar and {McMillan}, D. C. and Stallard, Sheila},
