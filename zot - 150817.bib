@article{makary_pancreaticoduodenectomy_2006,
	title = {Pancreaticoduodenectomy in the very elderly},
	volume = {10},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/16504879},
	abstract = {It is estimated that by 2050, there will be a 300\% increase in the elderly population ({\textgreater} or =65 years) and a corresponding increase in elderly patients presenting for surgical evaluation. Surgical decision-making in this population can be difficult because outcomes in the elderly are poorly defined. We reviewed 2698 consecutive pancreaticoduodenectomies ({PDs}) at our institution over a 35-year period (April 1970 through March 2005), with the last 1000 resections being done in the last 4 years. Data collected included surgical indication, mortality (defined as 30-day or in-hospital mortality), complications, and survival. Patients were divided by age into three groups ({\textless}80, 80-89, and {\textgreater} or =90 years) and evaluated using multiple logistic regression. Two hundred seven patients {\textgreater} or =80 years old underwent a {PD} (7.7\% of 2698). Patients 80-89 years of age had a mortality rate of 4.1\% (8 of 197) and a complication rate of 52.8\% (99 of 197), whereas patients {\textless} or =79 years of age had a mortality of 1.7\% and a complication rate of 41.6\% (P {\textless} 0.05). There were no perioperative deaths among the 10 patients {\textgreater} or =90 years of age, and their complication rate was 50\% (5 of 10). One-year survival for patients 80-89 years of age was 59.1\%, and that for patients {\textgreater} or =90 years was 60\%. Age was not an independent risk factor for perioperative mortality and morbidity following {PD} after adjusting for preoperative comorbidities. We demonstrate that {PD} can be safely performed in patients over 80 years of age and conclude that age alone should not be a contraindication to pancreatic resection. The advent of improved surgical outcomes and an aging population will likely result in a significant increase in the number of {PDs} performed in the next few decades.},
	pages = {347--56},
	number = {3},
	journaltitle = {J Gastrointest Surg},
	author = {Makary, Martin A and Winter, Jordan M and Cameron, John L and Campbell, Kurtis A and Chang, David and Cunningham, Steven C and Riall, Taylor S and Yeo, Charles J},
	date = {2006-03}
}

@book{quetelet_treatise_1842,
	title = {A Treatise on Man and the Development of His Faculties},
	isbn = {978-1-108-06442-2},
	abstract = {The Belgian polymath Lambert Adolphe Jacques Quetelet (1796-1874) pioneered social statistics. Applying his training in mathematics to the physical and psychological dimensions of individuals, he identified the 'average man' as characterised by the mean values of measured variables that follow a normal distribution. He believed that comparing the features of individuals against this average would allow scientists to better explore the processes that determine normal and abnormal qualities. Quetelet's methods influenced many, among them Florence Nightingale, and his simple measure for classifying a person's weight, dividing it by the square of their height, is widely known as the body mass index. First published in French in 1835 and reissued here in the 1842 English translation, this is his most influential work and includes a new preface that succinctly states his aim to be 'the analysis of normal man through his actions and of intellectual man through his productions'.},
	pagetotal = {145},
	publisher = {Cambridge University Press},
	author = {Quetelet, Lambert Adolphe Jacques and Smibert, T.},
	date = {1842},
	langid = {english},
	keywords = {Philosophy / Mind \& Body}
}

@article{seres_increased_2006,
	title = {Increased exercise capacity after surgically induced weight loss in morbid obesity},
	volume = {14},
	issn = {1930-7381},
	doi = {10.1038/oby.2006.35},
	abstract = {{OBJECTIVE}: To investigate the effects of surgically induced weight loss on exercise capacity in patients with morbid obesity ({MO}).
{RESEARCH} {METHODS} {AND} {PROCEDURES}: A prospective 1-year follow-up study was carried out, with patients being their own controls. A symptom-limited cardiopulmonary exercise stress test was performed in 31 {MO} patients ({BMI} {\textgreater} 40 kg/m2) before and 1 year after undergoing bariatric surgery.
{RESULTS}: At 1 year after surgery, weight was reduced from 146 +/- 33 to 95 +/- 19 kg (p {\textless} 0.001), and {BMI} went from 51 +/- 4 to 33 +/- 6 kg/m2 (p {\textless} 0.001). After weight loss, obese patients performed each workload with lower oxygen consumption, heart rate, systolic arterial pressure, and ventilatory volume (p {\textless} 0.001). This reduced energy expenditure allowed them to increase the duration of their effort test from 13.8 +/- 3.8 to 21 +/- 4.2 minutes (p {\textless} 0.001). Upon finishing the exercise, {MO} patients before surgery were able to reach only 83\% of their age-predicted maximal heart rate, and their respiratory exchange ratio was 0.87 +/- 0.06. After weight loss, those values were 90\% and 1 +/- 0.08, respectively (p {\textless} 0.01). When we compared the peak O2 pulse corrected by fat free mass before and after surgery, no significant differences between the groups were found.
{DISCUSSION}: After surgically induced weight loss, {MO} patients markedly improved their exercise capacity. This is due to the fact that they were able to perform the external work with lower energy expenditure and also to increase cardiovascular stress, optimizing the use of cardiac reserve. There were no differences in cardiac function before and after surgery.},
	pages = {273--279},
	number = {2},
	journaltitle = {Obesity (Silver Spring, Md.)},
	shortjournal = {Obesity (Silver Spring)},
	author = {Serés, Luis and Lopez-Ayerbe, Jordi and Coll, Ramón and Rodriguez, Oriol and Vila, Juan and Formiguera, Xavier and Alastrue, Antonio and Rull, Miguel and Valle, Vicente},
	date = {2006-02},
	pmid = {16571853},
	keywords = {Adult, Bariatric Surgery, Chi-Square Distribution, Energy Metabolism, Exercise, Exercise Test, Female, Follow-Up Studies, Humans, Male, Obesity, Morbid, Oxygen Consumption, Prospective Studies, Weight Loss}
}

