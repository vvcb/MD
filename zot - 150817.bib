@article{hilmy_relationship_2006,
	title = {The relationship between the systemic inflammatory response, tumour proliferative activity, T-lymphocytic infiltration and {COX}-2 expression and survival in patients with transitional cell carcinoma of the urinary bladder.},
	volume = {95},
	doi = {10.1038/sj.bjc.6603415},
	abstract = {The relationship between the systemic inflammatory response, tumour proliferative activity, T-lymphocytic infiltration, and {COX}-2 expression and survival was examined in patients with transitional cell carcinoma of the urinary bladder (n=103). Sixty-one patients had superficial disease and 42 patients had invasive  disease. Cancer-specific survival was shorter in those patients with invasive compared with superficial bladder cancer (P{\textless}0.001). On univariate analysis, stratified by stage, increased Ki-67 labelling index (P{\textless}0.05), increased {COX}-2 expression (P{\textless}0.05), C-reactive protein (P{\textless}0.05) and adjuvant therapy (P{\textless}0.01) were associated with poorer cancer-specific survival. On multivariate analysis of these significant factors, stratified by stage, only C-reactive protein ({HR} 2.89, 95\% {CI} 1.42-5.91, P=0.004) and adjuvant therapy ({HR} 0.29, 95\% {CI} 0.14-0.62, P=0.001) were independently associated with poorer cancer-specific survival. These results would suggest that tumour-based factors such as grade, {COX}-2 expression or T-lymphocytic infiltration are subordinate to systemic factors such as C-reactive protein in determining survival in patients with transitional cell  carcinoma of the urinary bladder.},
