@article{nakos_haemodynamics_1993,
	title = {Haemodynamics and gas exchange in liver cirrhosis: the effect of orally administered almitrine bismesylate},
	volume = {87},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/8497691},
	abstract = {The present study was conducted to analyse the effect on haemodynamics and to evaluate the role of hypoxic pulmonary vasoconstriction ({HPV}) in cirrhotic patients with hypoxaemia using almitrine bismesylate, an agent which increases the response of {HPV}. Six male patients, mean age of 51 years, with hepatic cirrhosis and associated hypoxaemia were studied. All patients had normal lung and cardiac function tests. When the patients were clinically stable, right heart and radial artery catheterization were performed. Data from the pulmonary artery catheter and blood gases were obtained before and after 4 days of oral almitrine bismesylate. The results indicated that these cirrhotics were in a mild hyperdynamic circulatory state. The cardiac output ({CO}), cardiac index ({CI}), oxygen delivery ({DO}2), and oxygen consumption ({VO}2) were elevated while the pulmonary vascular resistance ({PVR}) and systemic vascular resistance ({SVR}) were low. Almitrine significantly increased {PVR} (P {\textless} 0.05) and mean pulmonary artery pressure ({mPAP}) (P {\textless} 0.01). In addition, almitrine improved alveolar arterial oxygen content difference [D(A-a)O2] (P {\textless} 0.01) and shunt fraction (Qva/{QT}) (P {\textless} 0.01). The {PaCO}2 and {PaO}2 increased slightly but this was not statistically significant. In conclusion, in our group of patients with hepatic cirrhosis, four days of treatment with almitrine improved their gas exchange. The data suggest a weak {HPV} response in this group of cirrhotics; that response may be enhanced by almitrine.},
	pages = {93--8},
	number = {2},
	journaltitle = {Respir Med},
	author = {Nakos, G and Evrenoglou, D and Vassilakis, N and Lampropoulos, S},
	date = {1993-02},
	file = {Nakos et al. - 1993 - Haemodynamics and gas exchange in liver cirrhosis.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\UNBQWVVG\\Nakos et al. - 1993 - Haemodynamics and gas exchange in liver cirrhosis.pdf:application/pdf}
