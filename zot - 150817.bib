	keywords = {Aged, Colorectal Neoplasms, Exercise, Exercise Therapy, Exercise Tolerance, Female, Humans, Male, Middle Aged, Nutrition Therapy, Postoperative Care, preoperative care, Resistance Training, Single-Blind Method, Treatment Outcome, Walking}
}

