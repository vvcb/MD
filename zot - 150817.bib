	journaltitle = {Anesthesiology},
	author = {Song, Jian-Gang and Cao, Yun-Fei and Sun, Yu-Ming and Ge, Yan-Hu and Xu, Xue-Wu and Yang, Li-Qun and Liu, Zhi-Qiang and Song, Shao-Li and Yu, Wei-Feng},
