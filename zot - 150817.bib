@article{emick_hospital_2006,
	title = {Hospital readmission after pancreaticoduodenectomy},
	volume = {10},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/17114011},
	abstract = {Data exist on the morbidity and mortality of patients undergoing pancreaticoduodenectomy ({PD}), but there are few reports about hospital readmissions after this procedure. Our aim was to evaluate the number of and reasons for readmission after {PD} and the factors influencing readmission. We reviewed the initial hospitalization and readmissions for 1643 patients undergoing {PD} compared patients requiring readmission to patients that did not require readmission. Twenty-six percent of patients were readmitted a total of 678 times after {PD}. Patients readmitted were younger than those not readmitted (61.8 versus 64.6 years, P {\textless} 0.0001). Vessel resection, abscess formation, wound infection, postoperative percutaneous biliary stents, estimated blood loss {\textgreater}1000 ml, and age {\textless} or =65 years were independently associated with readmission. The length of stay for all patients decreased over time, from 10.5 days in 1996 to 7 days in 2003. The percentage of patients being readmitted also decreased from 33\% in 1996 to 20\% (P = 0.004) in 2003. The readmission rate after {PD} was 26\%. Younger age, blood loss, postoperative complications, and vessel resection were independent risk factors for readmission. The early hospital readmission rate has not increased in association with a decreased {LOS}, supporting the idea that reduction in {LOS} did not lead to increased readmission rates.},
	pages = {1243--52; discussion 1252--3},
	number = {9},
	journaltitle = {J Gastrointest Surg},
	author = {Emick, Dawn M and Riall, Taylor S and Cameron, John L and Winter, Jordan M and Lillemoe, Keith D and Coleman, Joann and Sauter, Patricia K and Yeo, Charles J},
	date = {2006-11}
}

@article{roxburgh_clinical_2014,
	title = {The clinical utility of the combination of T stage and venous invasion to predict survival in patients undergoing surgery for colorectal cancer.},
	volume = {259},
	doi = {10.1097/SLA.0000000000000229},
	abstract = {{OBJECTIVE}: To examine the clinical utility of improved detection of venous invasion ({VI}) in patients undergoing potentially curative resection of colorectal cancer. {BACKGROUND}: {VI} is a feature of colorectal cancer ({CRC}) progression. Elastica staining can be used to improve detection of {VI} and correspondingly its  prediction of patient survival. {METHODS}: A single-center, observational study of  pathology variables, including detection of {VI} by staining for elastica, using 631 stage I to {III} {CRC} specimens, collected from 1997 to 2009 (176 analyzed retrospectively and 455 analyzed prospectively), was performed. {RESULTS}: {VI} was detected in 56\% of patients with {CRC}. Over a median follow-up period of 73 months, 238 patients died (134 from cancer). On multivariate analysis, {VI} by elastica staining was associated with a shorter survival duration, independent of other pathology features, in all cases [hazard ratio ({HR}) = 3.94, 95\% confidence  interval ({CI}): 2.33-6.65, P {\textless} 0.001] and in node-negative cases ({HR} = 3.55, 95\% {CI}: 1.81-6.97; P {\textless} 0.001). In the absence of elastica-detected {VI}, with the exception of T stage, no other pathology features were associated with survival time. Therefore, the combination of T stage and {VI} ({TVI}) on survival was examined. Five-year cancer mortality could be stratified between 100\% and 54\% for patients with node-negative tumors and between 100\% and 33\% for patients with node-positive tumors. In all cases, the {TVI} had similar predictive value as that  of T stage and node status ({TNM}). In node-negative disease, {TVI} had superior predictive value. {CONCLUSIONS}: The results of the present study have prompted the development of a novel tumor staging system based on {TVI}. The {TVI} has clinical utility, especially in node-negative disease, in predicting outcome following curative resection for {CRC}.},
	pages = {1156--1165},
	number = {6},
	journaltitle = {Annals of surgery},
	shortjournal = {Ann Surg},
	author = {Roxburgh, Campbell S. D. and {McMillan}, Donald C. and Richards, Colin H. and Atwan, Manal and Anderson, John H. and Harvey, Tim and Horgan, Paul G. and Foulis, Alan K.},
	date = {2014-06},
	pmid = {24100338},
	keywords = {*Colectomy, *Veins, Aged, Colorectal Neoplasms/mortality/*pathology/surgery, Female, Follow-Up Studies, Great Britain/epidemiology, Humans, Laparotomy, Male, Neoplasm Invasiveness, Neoplasm Staging/*utilization, Prognosis, Retrospective Studies, Survival Rate/trends, Time Factors, Vascular Neoplasms/*pathology/surgery}
