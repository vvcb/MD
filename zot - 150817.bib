@article{mckernan_relationship_2008,
	title = {The relationship between quality of life ({EORTC} {QLQ}-C30) and survival in patients with gastro-oesophageal cancer.},
	volume = {98},
	doi = {10.1038/sj.bjc.6604248},
	abstract = {It remains unclear whether any aspect of quality of life has a role in predicting survival in an unselected cohort of patients with gastro-oesophageal cancer. Therefore the aim of the present study was to examine the relationship between quality of life ({EORTC} {QLQ}-C30), clinico-pathological characteristics and survival in patients with gastro-oesophageal cancer. Patients presenting with gastric or oesophageal cancer, staged using the {UICC} tumour node metastasis ({TNM}) classification and who received either potentially curative surgery or palliative treatment between November 1997 and December 2002 (n=152) participated in a quality of life study, using the {EORTC} {QLQ}-C30 core questionnaire. On univariate  analysis, age (P{\textless}0.01), tumour length (P{\textless}0.0001), {TNM} stage (P{\textless}0.0001), weight loss (P{\textless}0.0001), dysphagia score (P{\textless}0.001), performance status (P{\textless}0.1) and treatment (P{\textless}0.0001) were significantly associated with cancer-specific survival. {EORTC} {QLQ}-C30, physical functioning (P{\textless}0.0001), role functioning (P{\textless}0.001), cognitive functioning (P{\textless}0.01), social functioning (P{\textless}0.0001), global quality of  life (P{\textless}0.0001), fatigue (P{\textless}0.0001), nausea/vomiting (P{\textless}0.01), pain (P{\textless}0.001), dyspnoea (P{\textless}0.0001), appetite loss (P{\textless}0.0001) and constipation (P{\textless}0.05) were also significantly associated with cancer-specific survival. On multivariate survival  analysis, tumour stage (P{\textless}0.0001), treatment (P{\textless}0.001) and appetite loss (P{\textless}0.0001) were significant independent predictors of cancer-specific survival. The present study highlights the importance of quality of life ({EORTC} {QLQ}-C30) measures, in particular appetite loss, as a prognostic factor in these patients.},
	pages = {888--893},
	number = {5},
	journaltitle = {British journal of cancer},
	shortjournal = {Br J Cancer},
	author = {{McKernan}, M. and {McMillan}, D. C. and Anderson, J. R. and Angerson, W. J. and Stuart, R. C.},
	date = {2008-03-11},
