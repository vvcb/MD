	file = {Struthers et al. - 2008 - Assessing fitness for surgery a comparison of que.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\64FMXBEU\\Struthers et al. - 2008 - Assessing fitness for surgery a comparison of que.pdf:application/pdf}
