	number = {10},
	journaltitle = {International journal of cancer. Journal international du cancer},
	shortjournal = {Int J Cancer},
	author = {Leung, Elaine Y. L. and Crozier, Joseph E. M. and Talwar, Dinesh and O'Reilly, Denis St J. and {McKee}, Ruth F. and Horgan, Paul G. and {McMillan}, Donald C.},
