	pmid = {19628486},
	keywords = {Aged, Aortic Aneurysm, Abdominal, assessment, preanaesthetic, Exercise Test, Exercise Therapy, Female, Humans, Male, Middle Aged, Physical fitness, Pilot Projects, Prospective Studies, surgery, vascular, Treatment Outcome},
	file = {Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\VCT9CPUD\\Kothmann et al. - 2009 - Effect of short-term exercise training on aerobic .pdf:application/pdf;Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\M68QQ98J\\505.html:text/html}
