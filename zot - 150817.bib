	keywords = {*Leukocyte Count, *Lymphocyte Count, Aged, Colorectal Neoplasms/blood/mortality, Female, Humans, Male, Middle Aged, Neoplasms/*blood/mortality, Neutrophils/*immunology, Prognosis}
