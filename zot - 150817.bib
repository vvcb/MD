	number = {7},
	journaltitle = {Clinical Rehabilitation},
	shortjournal = {Clin Rehabil},
	author = {Dronkers, J. J. and Lamberts, H. and Reutelingsperger, Immd and Naber, R. H. and Dronkers-Landman, C. M. and Veldman, A. and Meeteren, {NLU} van},
	urldate = {2015-05-10},
	date = {2010-07-01},
	langid = {english},
