	author = {West, M. A. and Loughney, L. and Barben, C. P. and Sripadam, R. and Kemp, G. J. and Grocott, M. P. W. and Jack, S.},
	urldate = {2015-05-05},
	date = {2014-11},
	keywords = {Cancer surgery, Cardiopulmonary exercise testing, Morbidity, Neoadjuvant chemoradiotherapy, Physical fitness, rectal cancer},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\24KIMW4W\\West et al. - 2014 - The effects of neoadjuvant chemoradiotherapy on ph.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\P2KCN9NS\\S0748798314003862.html:text/html}
