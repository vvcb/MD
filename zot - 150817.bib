@article{wright_use_2008,
	title = {The use of orthopaedic {POSSUM} as an audit tool for fractured neck of femur},
	volume = {39},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/18316084},
	abstract = {In this prospective study, 230 patients who were operated on for a fractured neck of femur were evaluated using the physiological and operative severity score for the enumeration of mortality and morbidity ({POSSUM}). We observed 25 deaths (10.9\%) and 95 patients had a post-operative complication (41.3\%). The orthopaedic {POSSUM} equations yielded an overall predicted mortality of 21 patients and morbidity in 106 patients. This gave observed to expected (O:E) ratios of 1.19 and 0.95 respectively meaning that {POSSUM} agreed well with the observed mortality and morbidity. We concluded that {POSSUM} accurately predicts mortality and morbidity in patients with femoral neck fractures and when used as an audit tool would provide a fairer system of comparison and would allow an unbiased interpretation of results when changes in patient management are made.},
	pages = {430--5},
	number = {4},
	journaltitle = {Injury},
	author = {Wright, D M and Blanckley, S and Stewart, G J and Copeland, G P},
	date = {2008-04}
}

@article{crumley_elevated_2006,
	title = {An elevated C-reactive protein concentration, prior to surgery, predicts poor cancer-specific survival in patients undergoing resection for gastro-oesophageal  cancer.},
	volume = {94},
	doi = {10.1038/sj.bjc.6603150},
	abstract = {There is increasing evidence that the presence of an ongoing systemic inflammatory response is associated with poor outcome in patients undergoing resection for a variety of tumours. The aim of the present study was to examine the relationship between clinico-pathological status, preoperative C-reactive protein concentration and cancer-specific survival in patients undergoing resection for gastro-oesophageal cancer. One hundred and twenty patients attending the upper gastrointestinal surgical unit in the Royal Infirmary, Glasgow, who were selected for potentially curative surgery, were included in the study. Laboratory measurements of haemoglobin, white cell, lymphocyte and platelet counts, albumin and C-reactive protein were carried out at the time of diagnosis. All patients underwent en-bloc resection with lymphadenectomy and survived at least 30 days following surgery. On multivariate analysis, only the positive to total lymph node ratio (hazard ratio ({HR}) 2.02, 95\% confidence interval ({CI}) 1.44-2.84, P{\textless}0.001) and preoperative C-reactive protein concentration ({HR} 3.53, 95\% {CI} 1.88-6.64, P{\textless}0.001) were independent predictors of cancer-specific survival. The patient group with no evidence of a preoperative systemic inflammatory response (C-reactive protein {\textless} or =10 mg l(-1)) had a median survival of 79 months compared with 19 months in the elevated systemic inflammatory response group (P{\textless}0.001). The results of the present study indicate  that in patients selected to undergo potentially curative resection for gastro-oesophageal cancer, the presence of an elevated preoperative C-reactive protein concentration is an independent predictor of poor cancer-specific survival.},
	pages = {1568--1571},
	number = {11},
	journaltitle = {British journal of cancer},
	shortjournal = {Br J Cancer},
	author = {Crumley, A. B. C. and {McMillan}, D. C. and {McKernan}, M. and Going, J. J. and Shearer, C. J. and Stuart, R. C.},
	date = {2006-06-05},
	pmid = {16685271},
	pmcid = {PMC2361311},
	keywords = {Aged, C-Reactive Protein/*metabolism, Esophageal Neoplasms/*mortality/pathology/*surgery, Esophagogastric Junction, Female, Follow-Up Studies, Humans, Male, Middle Aged, Neoplasm Metastasis, Stomach Neoplasms/*mortality/pathology/*surgery, Survival Analysis, Time Factors}
