	number = {2},
	journaltitle = {European Journal of Surgical Oncology ({EJSO})},
	shortjournal = {European Journal of Surgical Oncology ({EJSO})},
	author = {Turrini, O. and Paye, F. and Bachellier, P. and Sauvanet, A. and Sa Cunha, A. and Le Treut, Y. P. and Adham, M. and Mabrut, J. Y. and Chiche, L. and Delpero, J. R.},
