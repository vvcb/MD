	date = {2010-11},
	pmid = {20676790},
	keywords = {Adenocarcinoma, Aged, Body Mass Index, Disease-Free Survival, Female, Humans, Male, Middle Aged, Neoplasm Recurrence, Local, Obesity, Morbid, Pancreatic Neoplasms, Pancreaticoduodenectomy, postoperative complications, Proportional Hazards Models, Survival Rate, Treatment Outcome},
