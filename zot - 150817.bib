@article{artinyan_infectious_2015,
	title = {Infectious postoperative complications decrease long-term survival in patients undergoing curative surgery for colorectal cancer: a study of 12,075 patients},
	volume = {261},
	issn = {1528-1140},
	doi = {10.1097/SLA.0000000000000854},
	shorttitle = {Infectious postoperative complications decrease long-term survival in patients undergoing curative surgery for colorectal cancer},
	abstract = {{OBJECTIVE}: We sought to characterize the effect of postoperative complications on long-term survival after colorectal cancer ({CRC}) resection.
{BACKGROUND}: The impact of early morbidity on long-term survival after curative-intent {CRC} surgery remains controversial.
{METHODS}: The Veterans Affairs Surgical Quality Improvement Program and Central Cancer Registry databases were linked to acquire perioperative and cancer-specific data for 12,075 patients undergoing resection for nonmetastatic {CRC} (1999-2009). Patients were categorized by presence of any complication within 30 days and by type of complication (noninfectious vs infectious). Univariate and multivariate survival analyses adjusted for patient, disease, and treatment factors were performed, excluding early deaths ({\textless}90 days). Subset analysis was performed to determine the specific impact of severe postoperative infections.
{RESULTS}: The overall morbidity and infectious complication rates were 27.8\% and 22.5\%, respectively. Patients with noninfectious postoperative complications were older, had lower preoperative serum albumin, had worse functional status, and had higher American Society of Anesthesiologists scores than patients with infectious complications and without complications (all P {\textless} 0.001). The presence of any complication was independently associated with decreased long-term survival [hazard ratio, 1.24; 95\% confidence interval (1.15-1.34)]. Multivariate analysis by complication type demonstrated increased risk only with infectious complications [hazard ratio, 1.31; 95\% confidence interval (1.21-1.42)]. Subset analysis demonstrated this effect predominantly in patients with severe infections [hazard ratio, 1.41; 95\% confidence interval (1.15-1.73)].
{CONCLUSIONS}: The presence of postoperative complications after {CRC} resection is associated with decreased long-term survival, independent of patient, disease, and treatment factors. The impact on long-term outcome is primarily driven by infectious complications, particularly severe postoperative infections.},
	pages = {497--505},
	number = {3},
	journaltitle = {Annals of Surgery},
	shortjournal = {Ann. Surg.},
	author = {Artinyan, Avo and Orcutt, Sonia T. and Anaya, Daniel A. and Richardson, Peter and Chen, G. John and Berger, David H.},
	date = {2015-03},
	pmid = {25185465},
	keywords = {Aged, Colorectal Neoplasms, Female, Hospitals, Veterans, Humans, Infection, Male, Neoplasm Staging, postoperative complications, prognosis, Registries, Risk Factors, Survival Rate}
}

@article{geer_prognostic_1993,
	title = {Prognostic indicators for survival after resection of pancreatic adenocarcinoma},
	volume = {165},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/8380315},
	abstract = {From October 1983 to October 1990, 799 patients with the diagnosis of pancreatic adenocarcinoma were admitted to Memorial Sloan-Kettering Cancer Center, and their records were entered into a prospective database. Curative resection was possible in 146 patients (18\%), with a 30-day operative mortality of 3.4\%. Median follow-up of survivors in the resection group was 28 months. Actuarial 5-year survival in patients who did not undergo resection was 0\% (n = 653), compared with 24\% in the patients who had resection (p {\textless} 0.00001). Of 52 patients available for 5-year follow-up, 10 (19\%) were alive at 5 years. Tumors with lymph node involvement, poor histologic tumor differentiation, and tumor size greater than 2.5 cm were predictors of a significantly worse survival in both univariate and multivariate analysis. Other factors, including blood transfusion, blood loss, and operative time, had no significant effect on survival. The majority of patients with pancreatic adenocarcinoma have unresectable disease with a poor prognosis. For patients who undergo curative resection, their prognosis appears to be determined by the biology of the tumor rather than factors involved in the resection.},
	pages = {68--72; discussion 72--3},
	number = {1},
