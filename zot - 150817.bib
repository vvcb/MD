	number = {8},
	journaltitle = {British Journal of Cancer},
	shortjournal = {Br J Cancer},
	author = {Moyes, L. H. and Leitch, E. F. and {McKee}, R. F. and Anderson, J. H. and Horgan, P. G. and {McMillan}, D. C.},
