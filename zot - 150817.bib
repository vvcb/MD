}

@article{crozier_relationship_2009,
	title = {Relationship between emergency presentation, systemic inflammatory response, and  cancer-specific survival in patients undergoing potentially curative surgery for  colon cancer.},
	volume = {197},
	doi = {10.1016/j.amjsurg.2007.12.052},
	abstract = {{BACKGROUND}: Emergency presentation is recognized to be associated with poorer cancer-specific survival following curative resection for colorectal cancer. The  present study examined the hypothesis that an enhanced systemic inflammatory response, prior to surgery, might explain the impact of emergency presentation on survival. {METHODS}: In all, 188 patients undergoing potentially curative resection for colorectal cancer were studied. Of these, 55 (29\%) presented as emergencies.  The systemic inflammatory response was assessed using the Glasgow Prognostic Score ({mGPS}), which is the combination of an elevated C-reactive protein ({\textgreater}10 mg/L) and hypoalbuminemia ({\textless}35 g/L). {RESULTS}: In the emergency group, tumor stage was greater (P {\textless} 0.01), more patients received adjuvant therapy (P {\textless} 0.01) more patients had an elevated {mGPS} (P {\textless} 0.01), and more patients died of their disease (P {\textless} 0.05). The minimum follow-up was 12 months; the median follow-up of the survivors was 48 months. Emergency presentation was associated with poorer 3-year cancer-specific survival in those patients aged 65 to 74 years (P {\textless} 0.01), in both males and females (P {\textless} 0.05), in the deprived (P {\textless} 0.01), in patients with tumor-node-metastasis ({TNM}) stage {II} disease (P {\textless} 0.01), in those who received no adjuvant therapy (P {\textless} 0.01), and in the {mGPS} 0 and 1 groups (P {\textless} 0.05) groups. On multivariate survival analysis of patients undergoing potentially curative surgery for {TNM} stage {II} colon cancer, emergency presentation (P {\textless} 0.05) and {mGPS} (P {\textless} 0.05) were independently associated with cancer-specific survival. {CONCLUSIONS}: These results suggest that emergency presentation and the presence of systemic inflammatory response prior to surgery are linked and account for poorer cancer-specific survival in patients undergoing potentially curative surgery for colon cancer. Both emergency presentation and an elevated {mGPS} should be taken into account when assessing the likely outcome of these patients.},
	pages = {544--549},
	number = {4},
	journaltitle = {American journal of surgery},
	shortjournal = {Am J Surg},
	author = {Crozier, Joseph E. M. and Leitch, E. Fiona and {McKee}, Ruth F. and Anderson, John H. and Horgan, Paul G. and {McMillan}, Donald C.},
	date = {2009-04},
	pmid = {18614139},
	keywords = {Aged, Colectomy, Colonic Neoplasms/blood/mortality/*surgery, C-Reactive Protein/analysis, Emergencies, Female, Humans, Hypoalbuminemia/diagnosis, Male, Middle Aged, Serum Albumin/analysis, Survival Analysis, Systemic Inflammatory Response Syndrome/blood}
}

@article{roxburgh_comparison_2010,
	title = {Comparison of the prognostic value of tumour- and patient-related factors in patients undergoing potentially curative surgery for colon cancer.},
	volume = {12},
	rights = {(c) 2010 The Authors. Colorectal Disease (c) 2010 The Association of Coloproctology of Great Britain and Ireland.},
	doi = {10.1111/j.1463-1318.2009.01961.x},
	abstract = {{AIM}: To comprehensively compare the prognostic value of tumour- and patient-related factors in patients undergoing curative surgery for colon cancer. {METHOD}: From a database of 287 patients who underwent elective resection between  1997 and 2005, tumour factors including stage and host factors including systemic inflammatory response [modified Glasgow Prognostic Score ({mGPS})] were identified. {RESULTS}: Median follow-up was 65 months. Over this period, 125 patients died, 80  from cancer. On multivariate analysis of all significant patient and tumour related factors, Dukes stage (P {\textless} 0.01), vascular invasion (P {\textless} 0.01) and the {mGPS} (P {\textless} 0.01) were independently associated with cancer-survival. Of the patient-related factors, age (P {\textless} 0.01), haemoglobin (P {\textless} 0.01), white-cell (P {\textless}  0.01), neutrophil (P {\textless} 0.01) and platelet (P {\textless} 0.01) counts, and alkaline phosphatase (P {\textless} 0.01) were most significantly associated with the {mGPS}. {CONCLUSION}: In addition to tumour-related factors such as Dukes stage and vascular invasion, the preoperative {mGPS} should be included to guide prognosis in patients undergoing curative resection for colon cancer.},
	pages = {987--994},
	number = {10},
	journaltitle = {Colorectal disease : the official journal of the Association of Coloproctology of Great Britain and Ireland},
	shortjournal = {Colorectal Dis},
	author = {Roxburgh, C. S. D. and Wallace, A. M. and Guthrie, G. K. and Horgan, P. G. and {McMillan}, D. C.},
