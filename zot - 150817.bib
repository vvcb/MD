	file = {Jones et al. - 2007 - Effects of presurgical exercise training on cardio.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\IQF8FI9U\\Jones et al. - 2007 - Effects of presurgical exercise training on cardio.pdf:application/pdf}
