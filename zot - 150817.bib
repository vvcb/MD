	langid = {english},
	keywords = {Body Composition, diabetes, fat, fetus, magnetic resonance imaging, pregnancy},
	file = {Anblagan et al. - 2013 - Measurement of fetal fat in utero in normal and di.pdf:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\6SCAMGDT\\Anblagan et al. - 2013 - Measurement of fetal fat in utero in normal and di.pdf:application/pdf;Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\UUH5WWWH\\abstract.html:text/html}
