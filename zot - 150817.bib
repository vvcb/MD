@article{west_cardiopulmonary_2014-1,
	title = {Cardiopulmonary exercise testing for the prediction of morbidity risk after rectal cancer surgery},
	volume = {101},
	rights = {© 2014 {BJS} Society Ltd. Published by John Wiley \& Sons Ltd},
