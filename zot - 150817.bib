@article{hennis_cardiopulmonary_2012,
	title = {Cardiopulmonary exercise testing predicts postoperative outcome in patients undergoing gastric bypass surgery},
	volume = {109},
	issn = {1471-6771},
	doi = {10.1093/bja/aes225},
	abstract = {{BACKGROUND}: For several types of non-cardiac surgery, the cardiopulmonary exercise testing ({CPET})-derived variables anaerobic threshold ({AT}), peak oxygen consumption ({VO}2 peak), and ventilatory equivalent for {CO}(2) ({VE}/{VCO}2 ) are predictive of increased postoperative risk: less physically fit patients having a greater risk of adverse outcome. We investigated this relationship in patients undergoing gastric bypass surgery.
{METHODS}: All patients ({\textless}190 kg) who were referred for {CPET} and underwent elective gastric bypass surgery at the Whittington Hospital {NHS} Trust between September 1, 2009, and February 25, 2011, were included in the study (n=121). Fifteen patients did not complete {CPET}. {CPET} variables ({VO}2 peak, {AT}, and {VE}/{VCO}2 ) were derived for 106 patients. The primary outcome variables were day 5 morbidity and hospital length of stay ({LOS}). The independent t-test and Fisher's exact test were used to test for differences between surgical outcome groups. The predictive capacity of {CPET} markers was determined using receiver operating characteristic ({ROC}) curves.
{RESULTS}: The {AT} was lower in patients with postoperative complications than in those without [9.9 (1.5) vs 11.1 (1.7) ml kg(-1) min(-1), P=0.049] and in patients with a {LOS}{\textgreater}3 days compared with {LOS} ≤ 3 days [10.4 (1.4) vs 11.3 (1.8) ml kg(-1) min(-1), P=0.023]. {ROC} curve analysis identified {AT} as a significant predictor of {LOS}{\textgreater}3 days ({AUC} 0.640, P=0.030). The {VO}2 peak and {VE}/{VCO}2 were not associated with postoperative outcome.
{CONCLUSIONS}: {AT}, determined using {CPET}, predicts {LOS} after gastric bypass surgery.},
	pages = {566--571},
	number = {4},
	journaltitle = {British Journal of Anaesthesia},
	shortjournal = {Br J Anaesth},
	author = {Hennis, P. J. and Meale, P. M. and Hurst, R. A. and O'Doherty, A. F. and Otto, J. and Kuper, M. and Harper, N. and Sufi, P. A. and Heath, D. and Montgomery, H. E. and Grocott, M. P. W.},
	date = {2012-10},
	pmid = {22810563},
	keywords = {Adult, anaerobic threshold, Area Under Curve, Body Weight, Exercise Test, Female, Gastric Bypass, Humans, length of stay, Male, Patient Readmission, Physical fitness, postoperative complications, Predictive Value of Tests, Prospective Studies, {ROC} Curve, Treatment Outcome}
}

