	number = {2},
	journaltitle = {Journal of Surgical Oncology},
	shortjournal = {J Surg Oncol},
	author = {Bathe, O. F. and Caldera, H. and Hamilton, K. L. and Franceschi, D. and Sleeman, D. and Livingstone, A. S. and Levi, J. U.},
