	number = {1},
	journaltitle = {European Journal of Vascular and Endovascular Surgery},
	author = {Young, E. L. and Karthikesalingam, A. and Huddart, S. and Pearse, R. M. and Hinchliffe, R. J. and Loftus, I. M. and Thompson, M. M. and Holt, P. J. E.},
	urldate = {2015-06-04},
	date = {2012-07-01},
	keywords = {{CPET}, exercise testing, Vascular Surgery},
	file = {Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\NQ3DMFHU\\Young et al. - 2012 - A Systematic Review of the Role of Cardiopulmonary.pdf:application/pdf;Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\K94BEZQV\\abstract.html:text/html}
