	number = {6},
	journaltitle = {World journal of surgery},
	shortjournal = {World J Surg},
	author = {Fearon, K. C. and Barber, M. D. and Falconer, J. S. and {McMillan}, D. C. and Ross, J. A. and Preston, T.},
