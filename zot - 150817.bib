	keywords = {Aged, Colorectal Neoplasms/*metabolism/mortality/therapy, Female, Humans, Inflammation Mediators/*metabolism, Leukocyte Count, Liver Neoplasms/metabolism/secondary, Male, Middle Aged, Neoplasm Staging, Prognosis, Survival Rate, Tumor Markers, Biological/*metabolism}
}

@article{lamb_relationship_2008,
	title = {The relationship between the local and systemic inflammatory responses and survival in patients undergoing resection for localized renal cancer.},
	volume = {102},
