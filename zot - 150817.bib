	number = {10},
	journaltitle = {European Journal of Cancer (Oxford, England: 1990)},
	shortjournal = {Eur. J. Cancer},
	author = {Vickers, M. M. and Powell, E. D. and Asmis, T. R. and Jonker, D. J. and Hilton, J. F. and O'Callaghan, C. J. and Tu, D. and Parulekar, W. and Moore, M. J.},
