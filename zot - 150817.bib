	author = {Chang, Chun-Ming and Su, Yu-Chieh and Lai, Ning-Sheng and Huang, Kuang-Yung and Chien, Sou-Hsin and Chang, Yu-Han and Lian, Wei-Cheng and Hsu, Ta-Wen and Lee, Ching-Chih},
	urldate = {2015-05-26},
	date = {2012-08-30},
	pmid = {22957007},
	pmcid = {PMC3431308},
	file = {PubMed Central Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\A6QJZMIK\\Chang et al. - 2012 - The Combined Effect of Individual and Neighborhood.pdf:application/pdf}
}

@article{misquita_applicability_2001,
	title = {Applicability of maximal oxygen consumption criteria in obese, postmenopausal women},
	volume = {10},
	issn = {1524-6094},
	doi = {10.1089/152460901753285787},
	abstract = {This study examines the applicability of using three standard criteria (age-predicted maximal heart rate [{HRmax}], respiratory exchange ratio [{RER}{\textgreater}1.10], and plateau in oxygen uptake [Vo(2)] for the measurement of maximal oxygen consumption (Vo(2)max) in postmenopausal women. One hundred eight postmenopausal (60 +/- 6 years), overweight and obese (body mass index [{BMI}] = 33 +/- 4 kg/m(2)), sedentary (Vo(2)max = 19 +/- 3 ml/kg/min) women underwent one exercise test (Bruce protocol), and 71 of these women underwent a second test (modified Balke protocol). On test 1, 69 (64\%) women achieved age-predicted {HRmax}, 61 (57\%) reached an {RER}{\textgreater}1.10, and 16 (15\%) achieved a plateau in Vo(2) ({\textless}2 ml/kg/min change). Women who reached age-predicted {HRmax} and reached an {RER} of at least 1.10 had a higher peak Vo(2) (p {\textless}0.01) than women who did not meet these criteria. There was no difference in the highest V02 obtained between women who did and did not achieve a plateau in Vo(2) during test 1 (p = 0.55). Resting {HR}, {HRmax}, and {RER} were similar between the two tests. On average, peak Vo(2) was higher on the second test (p {\textless}0.01). However, Vo(2)max was not different between exercise tests in women who achieved at least two of the three criteria on both tests (n = 24; test 1, 19.4 +/- 3.4; test 2: 19.8 +/- 3.7 ml/kg/min; p = {NS}). In addition, Vo(2)max was similar between the two exercise tests in 14 women who reached a plateau on the second test but did not reach a plateau on the first test (19.2 +/- 3.3 vs. 19.6 +/- 4.2 ml/kg/min; p = {NS}). We conclude that achievement of a plateau in Vo(2) is not a necessary criterion for a valid measurement of Vo(2)max in overweight and obese, sedentary, postmenopausal women.},
