	number = {9},
	journaltitle = {Journal of women's health \& gender-based medicine},
	shortjournal = {J Womens Health Gend Based Med},
	author = {Misquita, N A and Davis, D C and Dobrovolny, C L and Ryan, A S and Dennis, K E and Nicklas, B J},
