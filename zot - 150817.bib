@article{haupt_association_1997,
	title = {Association between preoperative acute phase response and postoperative complications},
	volume = {163},
	issn = {1102-4151},
	abstract = {{OBJECTIVE}: To find out if there is an association between the acute phase response preoperatively and the development of postoperative infective complications.
{DESIGN}: Prospective open study.
{SETTING}: Teaching hospital Germany.
{SUBJECTS}: 229 patients who were to undergo major abdominal operations.
