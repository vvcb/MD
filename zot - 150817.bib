	number = {4},
	journaltitle = {Journal of Gastrointestinal Surgery},
	shortjournal = {J Gastrointest Surg},
	author = {Hsu, Charles C. and Wolfgang, Christopher L. and Laheru, Daniel A. and Pawlik, Timothy M. and Swartz, Michael J. and Winter, Jordan M. and Robinson, Raymond and Edil, Barish H. and Narang, Amol K. and Choti, Michael A. and Hruban, Ralph H. and Cameron, John L. and Schulick, Richard D. and Herman, Joseph M.},
	urldate = {2015-05-26},
