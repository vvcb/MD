	urldate = {2015-08-04},
	date = {2015-07-31},
	pmid = {null},
	pmcid = {PMC4520073},
	file = {PubMed Central Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\WI5ADSQC\\Tjeertes et al. - 2015 - Obesity – a risk factor for postoperative complica.pdf:application/pdf}
}

