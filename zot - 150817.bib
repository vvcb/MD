	shortjournal = {Clinical Gastroenterology and Hepatology},
	author = {Sahora, Klaus and Ferrone, Cristina R. and Brugge, William R. and Morales-Oyarvide, Vicente and Warshaw, Andrew L. and Lillemoe, Keith D. and Fernández-del Castillo, Carlos},
	urldate = {2015-05-26},
	keywords = {cancer risk, management, pancreas, treatment},
	file = {ScienceDirect Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\VAKAZBHC\\S1542356515006369.html:text/html}
}

@article{mcmillan_changes_2000,
	title = {Changes in micronutrient concentrations following anti-inflammatory treatment in  patients with gastrointestinal cancer.},
	volume = {16},
	abstract = {Circulating concentrations of vitamin antioxidants (retinol, alpha-tocopherol, lutein, lycopene, alpha- and beta-carotene) and trace elements (zinc, copper, iron and selenium) plus carrier proteins (albumin, transferrin, caeruloplasmin) in gastrointestinal cancer patients (n = 12) with an inflammatory response (as demonstrated by an elevated C-reactive protein concentration) were compared with  a control group (n = 12). Further, the effect of moderating the inflammatory response, using the anti-inflammatory agent ibuprofen, on these measurements was  examined in the cancer group. The control and cancer groups were similar in terms of age, sex, and body mass index. However, the cancer group had significantly higher C-reactive protein concentrations (P {\textless} 0.001). Concentrations of vitamin antioxidants and trace elements (and carrier proteins) were significantly lower (P {\textless} 0.001), except copper (ceruloplasmin) which was significantly higher (P {\textless} 0.05). After anti-inflammatory treatment, there were small but significant increases in lutein, lycopene, and beta-carotene (P {\textless} 0.05) and in iron and selenium (P {\textless} 0.05), whereas ceruloplasmin decreased (P {\textless} 0. 05). The micronutrient concentrations in the cancer patients remained different from those in the control subjects. These results support the concept that the magnitude of  inflammation plays an important role in the regulation of circulating concentrations of vitamin antioxidants and trace elements in patients with gastrointestinal cancer.},
	pages = {425--428},
	number = {6},
