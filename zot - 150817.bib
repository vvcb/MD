	number = {17},
	journaltitle = {European journal of cancer (Oxford, England : 1990)},
	shortjournal = {Eur J Cancer},
	author = {Proctor, Michael J. and Morrison, David S. and Talwar, Dinesh and Balmer, Steven M. and Fletcher, Colin D. and O'Reilly, Denis St J. and Foulis, Alan K. and Horgan, Paul G. and {McMillan}, Donald C.},
	date = {2011-11},
