	langid = {english},
	keywords = {Aged, anaerobic threshold, Aortic Aneurysm, Abdominal, Electrocardiography, Ambulatory, Exercise Test, Female, Humans, Male, Middle Aged, Physical fitness, Pilot Projects, Reproducibility of Results},
	file = {Full Text PDF:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\M69AICK5\\Kothmann et al. - 2009 - Reliability of the anaerobic threshold in cardiopu.pdf:application/pdf;Snapshot:C\:\\Users\\vishnuvcb\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18snw64h.default\\zotero\\storage\\AI4BT2WT\\abstract.html:text/html}
