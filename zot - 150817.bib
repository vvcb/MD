	number = {4},
	journaltitle = {J Appl Physiol},
	author = {Robbins, Jennifer L and Duscha, Brian D and Bensimhon, Daniel R and Wasserman, Karlman and Hansen, James E and Houmard, Joseph A and Annex, Brian H and Kraus, William E},
